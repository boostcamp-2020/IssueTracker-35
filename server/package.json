{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "forever ./main/bin/www",
    "dev": "nodemon ./main/bin/www",
    "test": "jest --detectOpenHandles --forceExit",
    "lint": "eslint . --fix"
  },
  "jest": {
    "globalSetup": "<rootDir>/test/initTest.js",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/main/$1",
      "^@test/(.*)$": "<rootDir>/test/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "forever": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0"
  },
  "_moduleAliases": {
    "@": "./main",
    "@test": "./test",
    "@config": "./config"
  }
}